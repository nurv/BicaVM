DEFOP(INSTANCEOF)
    var indexbyte1 = READ_NEXT();
    var indexbyte2 = READ_NEXT();

    var objectref = OPPOP();
    if (objectref == null){ 
        OPPUSH(0); 
        break; 
    }
    var className = canonicalName(xl.constantPool.get((indexbyte1 << 8) | indexbyte2).name_ref);

    if(objectref["class"].isAssignable(jvm.classForName(className))){
        OPPUSH(1);
    }else{
        OPPUSH(0);
    }
ENDDEF

